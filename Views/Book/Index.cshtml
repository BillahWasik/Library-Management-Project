@model IEnumerable<BookModel>
@{
    ViewData["Title"] = "Index";
}

<div class="py-5 bg-light">
    <div class="container">
        <h3 class="h3">Top Books</h3>
        <div class="row">

            @foreach (var item in Model)
            {
            <div class="col-md-4">
                <partial name="_bookThumbnail" model="@item" />
            </div>
            }

               
            </div>
    </div>
</div>

